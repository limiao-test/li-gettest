package com.lmj.lei;
/**
 * 老师
 * 属性：  姓名  年龄   爱好  科目
 * 
 *行为：  介绍  教学   作业
 */
public class Teacher {
//	属性
	private String name="";
	private String age="";
	private String love="";
	private String subject="";
	
//	行为
	public void intro(){
		String info = "My name:" + name + "\nMy age:" + age + "\nMy love:" + love+"\nMysubject"+subject;
		System.out.println(info);
	}
	
}
